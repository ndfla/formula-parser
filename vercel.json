{  "builds": [
        { "src": "requirements.txt", "use": "@vercel/static" },
        { "src": "api/server.py", "use": "@vercel/python" },
        { "src": "api/util/*.py", "use": "@vercel/python" },
        { "src": "api/static/main.js", "use": "@vercel/node" },
        { "src": "api/static/module/AudioWorkletProcessor/wavetable-processor.js", "use": "@vercel/node" }

        
  ] ,
    "rewrites": [
        { 
            "source": "/(.*)", 
            "destination": "/api/server" 
        }
    ]
}